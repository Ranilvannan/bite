<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="crawl_book_sequence" model="ir.sequence">
            <field name="name">Crawl Book</field>
            <field name="code">crawl.book</field>
            <field name="prefix">BOOK/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="blog_article_sequence" model="ir.sequence">
            <field name="name">Blog Article</field>
            <field name="code">blog.article</field>
            <field name="prefix">Article/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="blog_photo_sequence" model="ir.sequence">
            <field name="name">Blog Photo</field>
            <field name="code">blog.photo</field>
            <field name="prefix">Photo/%(year)s/%(month)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

    </data>
</odoo>